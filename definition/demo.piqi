.custom-field doc

.alias [
    .name zeit
    .type time
]

.record [
    .name time
    .field [ .name hour .type uint32 ]
    .field [ .name minute .type uint32 ]
    .field [ .name second .type uint32 ]
]

.function [
    .name echo
    .doc "Echo Service"
    .input [
        .field [ .name in .type string .code 1 ]
    ]
    .output [
        .field [ .name out .type string .code 1 ]
        .field [ .name time .type time .code 2 ]
    ]
]
