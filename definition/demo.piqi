.import [ .module time ]
.import [ .module piqi ]

.custom-field doc

.alias [
    .name zeit
    .type time/time
]

.function [
    .name echo
    .doc "Echo Service"
    .input [
        .field [ .name in .type string .code 1 ]
    ]
    .output [
        .field [ .name out .type string .code 1 ]
        .field [ .name zeit .type zeit .code 2 ]
    ]
]
